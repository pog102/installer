#!/usr/bin/bash
rsync --progress -r ~/.bin/* ~/new_insta/bin/
rsync --progress -r ~/.config/* ~/new_insta/config/

rsync --progress -r ~/scripts/* ~/new_insta/scripts/

rsync --progress -r ~/Pictures/* ~/new_insta/Pictures/

rsync --progress -r  ~/.local/share/icons/custom/* ~/new_insta/icons/
rsync --progress -r  /etc/udev/rules.d/* ~/new_insta/rules/
cp ~/.zprofile ~/new_insta/zprofile
cp ~/.xinitrc ~/new_insta/xinitrc
cp ~/.xprofile ~/new_insta/xprofile


